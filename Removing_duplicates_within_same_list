def removeDuplicates(nums):
    count = 0
    og = len(nums)
    for each in range(0, len(nums)):
        if each != len(nums)-1:
            if nums[each] == nums[each+1]:
                nums[each]= None
                count += 1
        else:
            return nums

nums = [0,0,1,1,1,2,2,3,3,4]
nums = [x for x in removeDuplicates(nums) if x is not None]
print (nums)

